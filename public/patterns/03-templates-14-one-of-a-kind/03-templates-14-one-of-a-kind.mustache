&lt;div class=&quot;page l-header--fixed&quot;&gt;
  {{&gt; organisms-header }}
    &lt;div class=&quot;l-main&quot; role=&quot;main&quot;&gt;

        &lt;div class=&quot;page-title category-brand--western-redbud&quot;&gt;
          &lt;h1 class=&quot;page-title__title category-brand__text--knock-out&quot;&gt;{{ title }}&lt;/h1&gt;
        &lt;/div&gt;

        &lt;div class=&quot;l-container full-width&quot;&gt;
          {{&gt; molecules-breadcrumbs }}

          {{# emergency }}
            {{&gt; molecules-alert-error }}
          {{/ emergency }}

            &lt;div class=&quot;l-content&quot;&gt;
              &lt;div class=&quot;mason-grid&quot;&gt;
                &lt;div class=&quot;mason-grid__sizer&quot;&gt;&lt;/div&gt;
                &lt;div class=&quot;mason-grid__gutter&quot;&gt;&lt;/div&gt;
                {{# brand-textbox }}
                  &lt;div class=&quot;panel-pane mason-grid__item {{ brand-class }}&quot;&gt;
                    {{&gt; molecules-brand-textbox:brand-textbox--large-text }}
                  &lt;/div&gt;
                {{/ brand-textbox }}
                &lt;div class=&quot;panel-pane mason-grid__item mason-grid__item--width2&quot;&gt;
                    {{&gt; molecules-video-youtube(video-id: &quot;PAwB_t_iM7U&quot;, video-width: &quot;853&quot;, video-height: &quot;480&quot;) }}
                &lt;/div&gt;
                {{# profile-highlight }}
                    &lt;div class=&quot;panel-pane mason-grid__item&quot;&gt;
                      {{&gt; molecules-profile-highlight }}
                    &lt;/div&gt;
                {{/ profile-highlight }}
              &lt;/div&gt;
            &lt;/div&gt;&lt;!--end .l-content--&gt;

        &lt;/div&gt;
    &lt;/div&gt;&lt;!--End role=main--&gt;
  {{&gt; organisms-footer }}
&lt;/div&gt;