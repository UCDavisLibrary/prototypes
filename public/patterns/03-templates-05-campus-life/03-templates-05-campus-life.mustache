&lt;div class=&quot;page l-header--fixed&quot;&gt;
  {{&gt; organisms-header }}
    &lt;div class=&quot;l-main&quot; role=&quot;main&quot;&gt;

        &lt;div class=&quot;page-title category-brand--western-redbud page-title--banner&quot;&gt;
          &lt;div class=&quot;page-title__title--nested&quot;&gt;
            &lt;h1 class=&quot;page-title__title category-brand__text--knock-out&quot;&gt;{{ title }}&lt;/h1&gt;
          &lt;/div&gt;
          {{&gt; atoms-title-banner }}
        &lt;/div&gt;

        &lt;div class=&quot;l-container brenham-flipped&quot;&gt;
          {{&gt; molecules-breadcrumbs }}

          {{# emergency }}
            {{&gt; molecules-alert-error }}
          {{/ emergency }}

            &lt;div class=&quot;l-content&quot;&gt;
              &lt;div class=&quot;box-wrapper&quot;&gt;
                &lt;div class=&quot;panel-pane body-text&quot;&gt;
                  &lt;h2 class=&quot;heading--underline&quot;&gt;{{ headline.short }}&lt;/h2&gt;
                  {{{ excerpt.long }}}
                &lt;/div&gt;
              &lt;/div&gt;
              &lt;div class=&quot;mason-grid&quot;&gt;
                &lt;div class=&quot;mason-grid__sizer&quot;&gt;&lt;/div&gt;
                &lt;div class=&quot;mason-grid__gutter&quot;&gt;&lt;/div&gt;
                {{# marketing-highlight.short-burst }}
                  &lt;div class=&quot;panel-pane mason-grid__item&quot;&gt;
                    {{&gt; molecules-marketing-highlight }}
                  &lt;/div&gt;
                {{/ marketing-highlight.short-burst }}
                &lt;div class=&quot;panel-pane mason-grid__item mason-grid__item--width2&quot;&gt;
                    {{&gt; molecules-featured-video }}
                &lt;/div&gt;
                {{# marketing-highlight.long-burst }}
                    &lt;div class=&quot;panel-pane mason-grid__item&quot;&gt;
                      {{&gt; molecules-marketing-highlight }}
                    &lt;/div&gt;
                {{/ marketing-highlight.long-burst }}
              &lt;/div&gt;
            &lt;/div&gt;&lt;!--end .l-content--&gt;



            &lt;div class=&quot;l-sidebar-second&quot;&gt;
                &lt;div class=&quot;panel-pane&quot;&gt;
                    &lt;div class=&quot;sub-nav&quot;&gt;
                      {{# tertiary-page }}
                          &lt;h2 class=&quot;sub-nav__title&quot;&gt;&lt;a href=&quot;{{ url }}&quot;&gt;{{ parent-page-title }}&lt;/a&gt;&lt;/h2&gt;
                      {{/ tertiary-page }}
                        &lt;ul class=&quot;menu sub-nav__body&quot;&gt;
                          &lt;li&gt;&lt;a href=&quot;{{ url }}&quot;&gt;Campus Features&lt;/a&gt;&lt;/li&gt;
                          &lt;li&gt;&lt;a href=&quot;{{ url }}&quot;&gt;Housing &amp;amp; Dining&lt;/a&gt;&lt;/li&gt;
                          &lt;li&gt;&lt;a href=&quot;{{ url }}&quot;&gt;Clubs &amp;amp; Organizations&lt;/a&gt;&lt;/li&gt;
                          &lt;li&gt;&lt;a href=&quot;{{ url }}&quot;&gt;Athletics&lt;/a&gt;&lt;/li&gt;
                          &lt;li&gt;&lt;a href=&quot;{{ url }}&quot;&gt;Cultural Life&lt;/a&gt;&lt;/li&gt;
                        &lt;/ul&gt;
                    &lt;/div&gt;
                &lt;/div&gt;
                &lt;div class=&quot;panel-pane box&quot;&gt;
                    {{&gt; organisms-events }}
                &lt;/div&gt;
                {{# focal-links }}
                    &lt;div class=&quot;panel-pane&quot;&gt;
                      {{&gt; molecules-focal-link }}
                    &lt;/div&gt;
                {{/ focal-links }}
                &lt;div class=&quot;panel-pane&quot;&gt;
                    {{&gt; organisms-facebook-activity }}
                &lt;/div&gt;
                &lt;div class=&quot;panel-pane bg-transparent&quot;&gt;
                  {{&gt; molecules-social-follow-small }}
                &lt;/div&gt;
            &lt;/div&gt;&lt;!--end .l-sidebar-second--&gt;

        &lt;/div&gt;
    &lt;/div&gt;&lt;!--End role=main--&gt;
  {{&gt; organisms-footer }}
&lt;/div&gt;