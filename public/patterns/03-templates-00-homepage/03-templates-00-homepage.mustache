&lt;div class=&quot;page l-header--fixed&quot;&gt;
  {{&gt; organisms-header }}
  &lt;div class=&quot;l-main brenham&quot; role=&quot;main&quot;&gt;

    &lt;div class=&quot;panel-pane panel-pane--flush&quot;&gt;
      {{# hero-banner }}
        {{&gt; organisms-hero-banner}}
      {{/ hero-banner }}
    &lt;/div&gt;

    &lt;div class=&quot;panel-pane&quot;&gt;
      &lt;div class=&quot;l-container&quot;&gt;
        {{&gt; organisms-priority-links }}
      &lt;/div&gt;
    &lt;/div&gt;

    &lt;div class=&quot;l-container&quot;&gt;

      {{# emergency }}
        {{&gt; molecules-alert-error }}
      {{/ emergency }}

      &lt;div class=&quot;l-content&quot;&gt;
        &lt;div class=&quot;panel-pane&quot;&gt;
          {{&gt; molecules-spotlight-article:category-brand--western-redbud }}
        &lt;/div&gt;
        &lt;div class=&quot;panel-pane bg-transparent&quot;&gt;
          {{&gt; organisms-achievements }}
        &lt;/div&gt;
        &lt;div class=&quot;panel-pane&quot;&gt;
          {{&gt; molecules-slideshow }}
        &lt;/div&gt;
        {{# marketing-highlight-featured.short-burst }}
          &lt;div class=&quot;panel-pane l-sibling-grid--half&quot;&gt;
            {{&gt; molecules-marketing-highlight:marketing-highlight--featured }}
          &lt;/div&gt;
        {{/ marketing-highlight-featured.short-burst }}
        &lt;div class=&quot;panel-pane&quot;&gt;
          {{&gt; molecules-video-youtube(video-id: &quot;nZZDAzErkIU&quot;, video-width: &quot;853&quot;, video-height: &quot;480&quot;) }}
        &lt;/div&gt;
        &lt;div class=&quot;panel-pane bg-transparent&quot;&gt;
          {{&gt; molecules-social-follow }}
        &lt;/div&gt;
      &lt;/div&gt;&lt;!--end .l-content--&gt;

      &lt;div class=&quot;l-sidebar-first&quot;&gt;
        &lt;div class=&quot;panel-pane box&quot;&gt;
          {{&gt; organisms-trending }}
        &lt;/div&gt;
        &lt;div class=&quot;panel-pane box&quot;&gt;
          {{&gt; organisms-events }}
        &lt;/div&gt;
        &lt;div class=&quot;panel-pane box&quot;&gt;
          {{&gt; organisms-davis-athletics }}
        &lt;/div&gt;
        &lt;div class=&quot;panel-pane box&quot;&gt;
          {{&gt; organisms-student-blog }}
        &lt;/div&gt;
        {{# focal-links }}
          &lt;div class=&quot;panel-pane&quot;&gt;
            {{&gt; molecules-focal-link }}
          &lt;/div&gt;
        {{/ focal-links }}
      &lt;/div&gt;&lt;!--end .l-sidebar-first--&gt;

    &lt;/div&gt;
  &lt;/div&gt;&lt;!--End role=main--&gt;
  {{&gt; organisms-footer }}
&lt;/div&gt;
