&lt;div class=&quot;page&quot;&gt;
  {{&gt; organisms-header }}
    &lt;div class=&quot;l-main&quot; role=&quot;main&quot;&gt;

        &lt;div class=&quot;page-title&quot;&gt;
            &lt;h1 class=&quot;page-title__title category-brand__text--knock-out&quot;&gt;{{ title }}&lt;/h1&gt;
        &lt;/div&gt;

        &lt;div class=&quot;l-container full-width&quot;&gt;
          {{&gt; molecules-breadcrumbs }}

            &lt;div class=&quot;l-content&quot;&gt;
                &lt;div class=&quot;box-wrapper&quot;&gt;
                    &lt;div class=&quot;panel-pane&quot;&gt;
                        &lt;ul class=&quot;list--bordered list--flush&quot;&gt;
                          {{# listItems.ten }}
                            &lt;li&gt;
                              &lt;h4&gt;&lt;a href=&quot;#&quot;&gt;{{ name.first }} {{ name.last }}&lt;/a&gt;&lt;/h4&gt;
                              &lt;p&gt;{{ description }}&lt;/p&gt;
                            &lt;/li&gt;
                          {{/ listItems.ten }}
                        &lt;/ul&gt;
                        {{&gt; molecules-pagination }}
                    &lt;/div&gt;
                &lt;/div&gt;&lt;!--end .l-content--&gt;
            &lt;/div&gt;&lt;!--end .l-content--&gt;

        &lt;/div&gt;
    &lt;/div&gt;&lt;!--End role=main--&gt;
  {{&gt; organisms-footer }}
&lt;/div&gt;