<div class="page l-header--fixed">
  {{> organisms-header }}
  <div class="l-main" role="main">

    <div class="page-title">
      <h1 class="page-title__title category-brand__text--knock-out">{{ title }}</h1>
    </div>

    <div class="l-container brenham-flipped">
      {{> molecules-breadcrumbs }}

      {{# emergency }}
        {{> molecules-alert-error }}
      {{/ emergency }}

      <div class="l-content">
        <div class="box-wrapper">
          <div class="panel-pane category-brand--sky-blue">
            {{> molecules-focus-box }}
          </div>
          <div class="panel-pane">
            <h2 class="heading--underline">{{ headline.short }}</h2>
            <div class="align-left image-half">
              {{> atoms-landscape-4x3 }}
            </div>
            {{{ excerpt.long }}}
          </div>
          {{# collapse }}
            <div class="panel-pane">
              {{> molecules-collapse }}
            </div>
          {{/ collapse }}
        </div>
      </div><!--end .l-content-->

      <div class="l-sidebar-second">
        <div class="panel-pane">
          <div class="sub-nav">
            {{# tertiary-page }}
                <h2 class="sub-nav__title"><a href="{{ url }}">{{ parent-page-title }}</a></h2>
            {{/ tertiary-page }}
              <ul class="menu sub-nav__body category-brand--admin-blue">
                  <li><a href="{{ url }}">Our Story</a></li>
                  <li><a href="{{ url }}">Facts &amp; Rankings</a></li>
                  <li><a href="{{ url }}">Visit UC Davis</a></li>
                  <li><a href="{{ url }}">Locations/Parking</a></li>
                  <li class="sub-nav__body--active category-brand__list-link"><a href="{{ url }}">Administration</a>
                      <ul>
                          <li><a href="{{ url }}">Major Admin Units</a></li>
                          <li><a href="{{ url }}">Organizational Chart</a></li>
                          <li><a href="{{ url }}">Initiatives</a></li>
                          <li><a href="{{ url }}">Policies</a></li>
                          <li><a href="{{ url }}">Executive Recruitments</a></li>
                      </ul>
                  </li>
                  <li><a href="{{ url }}">Jobs</a></li>
              </ul>
          </div>
        </div>
        <div class="panel-pane box">
          {{> organisms-initiatives-feed }}
        </div>
        {{# focal-links }}
          <div class="panel-pane">
            {{> molecules-focal-link }}
          </div>
        {{/ focal-links }}
        <div class="panel-pane">
          {{> organisms-admin-twitter-activity }}
        </div>
        <div class="panel-pane bg-transparent">
          {{> molecules-social-follow-small }}
        </div>
      </div><!--end .l-sidebar-second-->

    </div>
  </div><!--End role=main-->
  {{> organisms-footer }}
</div>