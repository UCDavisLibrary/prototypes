<div class="page" id="page">
	{{> organisms-wine-header }}
	<div role="main">
		<div class="l-two-col">
			<div class="l-main">
				<article class="article">
					<header class="article-header">
						{{> molecules-headline }}
						{{> molecules-byline }}
					</header>
					{{> organisms-article-body }}
				</article><!--end .article-->
				{{> molecules-social-share }}
				{{> organisms-comment-thread }}
			</div><!--end l-main-->

			<div class="l-sidebar">
				{{> organisms-related-posts }}
				{{> organisms-recent-tweets }}
			</div><!--end l-sidebar-->
		</div><!--end l-two-col-->	
	</div><!--End role=main-->
	{{> organisms-footer }}
</div>